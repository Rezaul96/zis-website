<section class="subscribe-one">
    <div class="container">
        <div class="subscribe-one__inner">
            <div class="subscribe-one__shape-1 float-bob-y">
                <img src="~/website/assets/images/shapes/subscribe-one-shape-1.png" alt="">
            </div>
            <div class="subscribe-one__shape-2 float-bob-x">
                <img src="~/website/assets/images/shapes/subscribe-one-shape-2.png" alt="">
            </div>
            <div class="subscribe-one__shape-3 float-bob-y">
                <img src="~/website/assets/images/shapes/subscribe-one-shape-3.png" alt="">
            </div>
            <div class="subscribe-one__shape-4 float-bob-x">
                <img src="~/website/assets/images/shapes/subscribe-one-shape-4.png" alt="">
            </div>
            <div class="row">
                <div class="col-xl-6">
                    <div class="subscribe-one__left">
                        <h3 class="subscribe-one__title">
                            Start your free trial
                            right away.
                        </h3>
                    </div>
                </div>
                <div class="col-xl-6">
                    <div class="subscribe-one__right">
                        <form class="subscribe-one__form mc-form" data-url="MC_FORM_URL">
                            <div class="subscribe-one__input-box">
                                <input type="email" placeholder="Email address" name="email">
                                <button type="submit" class="subscribe-one__btn">Subscribe</button>
                            </div>
                        </form>
                        <div class="mc-form__response"></div>
                        <p class="subscribe-one__text">Subsrcibe for our upcoming latest articles</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>